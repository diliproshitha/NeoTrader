{
    "manifest_version": 2,
    "name": "Tradingview To Metatrader",
    "version": "1.0",
    "description": "Send orders from Tradingview to Metatrader platform by one click.",
    "icons": {
        "48": "icons/icon-48.png",
        "96": "icons/icon-96.png"
    },
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_icon": "icons/icon-48.png",
        "theme_icons": [
            {
                "light": "icons/icon-48.png",
                "dark": "icons/icon-48.png",
                "size": 32
            }
        ],
        "default_title": "TradingviewToMetatrader",
        "default_popup": "src/popup/index.html"
    }
}